{
  "name": "theia-extension-tester",
  "version": "0.1.0",
  "description": "A testing library built on Selenium to test Eclipse Theia extensions / plugins. Eclipse Che is supported as well.",
  "keywords": [
    "che",
    "Eclipse Che",
    "Eclipse Theia",
    "extension",
    "plugin",
    "selenium",
    "testing",
    "theia",
    "theia-extension-tester",
    "webdriver"
  ],
  "author": "Marian Lorinc <mlorinc@redhat.com>",
  "homepage": "https://github.com/mlorinc/theia-extension-tester",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "theiatest": "./lib/cli.js"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.com/"
  },
  "scripts": {
    "prepublish": "yarn run clean && yarn run build",
    "test": "echo \"Error: run tests from root\" && exit 1",
    "build": "tsc",
    "build:starter-module": "tsc",
    "clean": "rimraf lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mlorinc/theia-extension-tester.git",
    "directory": "packages/theia-extension-tester"
  },
  "dependencies": {
    "@theia-extension-tester/adapters": "0.1.0",
    "@theia-extension-tester/base-browser": "0.1.0",
    "@theia-extension-tester/browser-runner": "0.1.0",
    "@theia-extension-tester/che-browser": "0.1.0",
    "@theia-extension-tester/che-factory-runner": "0.1.0",
    "@theia-extension-tester/openshift-authenticator": "0.1.0",
    "@theia-extension-tester/page-objects": "0.1.0",
    "@theia-extension-tester/theia-browser": "0.1.0",
    "commander": "8.2.0",
    "dotenv": "10.0.0",
    "extension-tester-page-objects": "0.1.1",
    "fs-extra": "10.0.0"
  }
}
